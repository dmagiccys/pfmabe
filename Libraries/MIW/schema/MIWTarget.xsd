						</xsd:attribute>
						<xsd:attribute name="sspLongitude" type="Longitude_Type">
							<xsd:annotation>
								<xsd:documentation>The longitude coordinate at which the sound speed profile was taken. Measured in Degrees.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="sspDTG" type="xsd:dateTime">
							<xsd:annotation>
								<xsd:documentation>THE CALENDAR DATE AND TIME at which the  sound speed profile was taken.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="sspSource" type="sourceCode">
							<xsd:annotation>
								<xsd:documentation>The code identifing the source of the sound speed data.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="sspType" type="sourceType">
							<xsd:annotation>
								<xsd:documentation>The code identifing the type of the sound speed data.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
