						</xsd:attribute>
						<xsd:attribute name="bottomCompCode">
							<xsd:annotation>
								<xsd:documentation>The code identifing the ocean floor composition.
   0-Mud
   1-Sand 
   2-Gravel
   3-Rock
255-Unknown</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:integer">
									<xsd:enumeration value="0"/>
									<xsd:enumeration value="1"/>
									<xsd:enumeration value="2"/>
