									<xsd:enumeration value="4"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="bottomCompSource" type="sourceCode">
							<xsd:annotation>
								<xsd:documentation>The code identifing the source of the bottom composition data.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="bottomCompType" type="sourceType">
							<xsd:annotation>
								<xsd:documentation>The code identifing the type of the bottom composition data.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="bottomDescCode" type="xsd:integer">
							<xsd:annotation>
								<xsd:documentation>The code identifing the description of the ocean floor.
   0-Clay
   1-Very Fine Silt
   2-Fine Silt
   3-Silt
   4-Coarse Silt
   5-Very fine Sand
   6-Fine Sand
   7-Sand
   8-Course Sand
   9-Very Course Sand
  10-Granules
   11-Pebbles/Shells
   12-Oyster SHells/Cobbles
   13-Boulders
   14-Solid Rock
  255-Unknown

</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="bottomDescSource" type="sourceCode">
							<xsd:annotation>
								<xsd:documentation>The code identifing the source of the bottom description data.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="bottomDescType" type="sourceType">
							<xsd:annotation>
								<xsd:documentation>The code identifing the type of the bottom description data.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="bottomRoughnessCode" type="xsd:integer">
							<xsd:annotation>
								<xsd:documentation>The code identifing the general type of ocean floor where the object was identified.
   0-Smooth
   1-Moderate
   2-Rough
   255-Unknown</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="bottomRoughnessSource" type="sourceCode">
							<xsd:annotation>
								<xsd:documentation>The code identifing the source of the bottom roughness data.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="bottomRoughnessType" type="sourceType">
