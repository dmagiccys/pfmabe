					<xsd:complexType>
						<xsd:complexContent>
							<xsd:extension base="xsd:anyType">
								<xsd:attribute name="representationType" use="required">
									<xsd:annotation>
										<xsd:documentation>The type specifying the type of representation. 0= line 1 = polygon</xsd:documentation>
									</xsd:annotation>
									<xsd:simpleType>
										<xsd:restriction base="xsd:integer">
											<xsd:enumeration value="0"/>
											<xsd:enumeration value="1"/>
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:attribute>
								<xsd:attribute name="count" use="required">
									<xsd:annotation>
										<xsd:documentation>The number of vertices in the representation."</xsd:documentation>
									</xsd:annotation>
									<xsd:simpleType>
										<xsd:restriction base="xsd:integer"/>
									</xsd:simpleType>
								</xsd:attribute>
							</xsd:extension>
						</xsd:complexContent>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="length">
				<xsd:annotation>
					<xsd:documentation>The length dimension of the contact for the Target. Units are in meters.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal"/>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="width" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>The width dimension of the contact for the Target. Units are in meters.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="height" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>The height dimension of the contact from the bottom. Measured in Meters.
</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="depth" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>The depth dimension from the water surface to the contact. Measured in Meters.
</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="horizontalOrientation">
				<xsd:annotation>
					<xsd:documentation>The bearing angle, measured from true North, to the major horizontal axis of the contact. Measerued in Angular Degrees.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0.0"/>
						<xsd:maxInclusive value="360.0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="verticalOrientation">
				<xsd:annotation>
					<xsd:documentation>The angle measured from the sea floor to the major vertical axis of the contact. Measured in Angular Degrees.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0.0"/>
						<xsd:maxInclusive value="90.0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="ringing">
				<xsd:annotation>
					<xsd:documentation>The code that denotes a strong signal return on a sonar image.
   0=No
   1=Yes</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="1"/>
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="N"/>
						<xsd:enumeration value="Y"/>
						<xsd:enumeration value="T"/>
						<xsd:enumeration value="F"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="strength">
				<xsd:annotation>
					<xsd:documentation>The code that denotes the return echo strength of a contact.
0=Strong
1=Medium
2=Weak</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="shadow">
				<xsd:annotation>
					<xsd:documentation>The code that denotes that thecontact  is proud of bottom.
   0=No
   1=Yes</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="1"/>
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="N"/>
						<xsd:enumeration value="Y"/>
						<xsd:enumeration value="T"/>
						<xsd:enumeration value="F"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="description" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The text describing the contact.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute ref="remarks"/>
			<xsd:attribute name="reasonForUpdate" type="xsd:integer"/>
			<xsd:attribute name="confidenceLevel">
				<xsd:annotation>
					<xsd:documentation>The code that denotes the amount of confidence the analyst has in the values entered.

   0=N/A
   1=Low
   2=Medium Low
   3=Medium
   4=Medium High
   5=High</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
						<xsd:enumeration value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="surveyId" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The name of the exercise or survey operation which first identified the contact.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute ref="latitude" use="required">
				<xsd:annotation>
					<xsd:documentation>The latitude coordinate of the contact.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute ref="longitude" use="required">
				<xsd:annotation>
					<xsd:documentation>The longitude coordinate of the contact.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="eventDTG" type="xsd:dateTime" use="required">
				<xsd:annotation>
					<xsd:documentation>The THE CALENDAR DATE AND TIME (UTC) of the observation of the contact.  See W3C XML Schema Specifiactions for format details.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="LRN" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The identifier that represents Lay Reference Number (LRN) assigned to the contact by the identifying unit.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="errorMajorAxis" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>The length of the  majoraxis of an elliptical area defining the possible error in the horizontal position of the contact. Measured in Meters.
</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="errorMinorAxis" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>The length of the minor axis of an elliptical area defining the possible error in the horizontal position of the contact. Measured in Meters.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="errorAngle">
				<xsd:annotation>
					<xsd:documentation>The bearing angle, from true North, to the major axis of an elliptical area defining the possible error in the position of the contact.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0.0"/>
						<xsd:maxInclusive value="180.0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="surveyType">
				<xsd:annotation>
					<xsd:documentation>The code that denotes the organizational activity conducting the survey.
0=MCM
1=NAVOCEANO
2=Other</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="class">
				<xsd:annotation>
					<xsd:documentation>The code that denotes the class of the contact.
0=Mine Like
1=Non-mine Like
2=Unexploded</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="category">
				<xsd:annotation>
					<xsd:documentation>The code that denotes the category describing a contact.
0=Coral Head
1=Fish Equipment/Trap
2=Pipe Drum
3=Rock
4=Scrap Medal
5=Whip Wreckage
6=Well Head
7=Other Man-made
8=Other Natural
</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
						<xsd:enumeration value="5"/>
						<xsd:enumeration value="6"/>
						<xsd:enumeration value="7"/>
						<xsd:enumeration value="8"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="shape" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The text that describes the geometric shape of the contact.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="burial">
				<xsd:annotation>
					<xsd:documentation>The percentage of the contact buried.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="100"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="multipleAspects">
				<xsd:annotation>
					<xsd:documentation>The code that denotes whether the contact has been detected from multiple angles.
0=No
1=Yes</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="status" type="xsd:string"/>
			<xsd:attribute name="fusionId" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>This is the identifier of a object fused from other objects.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="historyRecordNumber" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>The record number of the last observation of an object (i.e. the currnt info on a particular contact).</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="analystActivity" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The name of the organization of then analyst that created the Object.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="analyst" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The name of the analyst who created the contact.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="RS_analyst" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The name of the analyst who put the contact in the MCDB or Route Survey DB. (MIW Only)</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="averageError" type="xsd:integer"/>
			<xsd:attribute name="standardDeviation" type="xsd:integer"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Sonar">
		<xsd:annotation>
			<xsd:documentation>The Sonar parameters at the time of an observation of an object.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:attribute ref="name">
				<xsd:annotation>
					<xsd:documentation>he name of the equipment used in the event.  (SQQ-30, SQQ-32, Klein 100, Klein  500, DUBM20,...)</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="mode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The code identifing the mode of the sonar when the object was identified.
  0=Detect
  1=Classify</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="type">
				<xsd:annotation>
					<xsd:documentation>The code identifing the type of sonar used to identy the object.
   0=Ahead Looking
   1=Hand Held
   2=SideScan
   3=Other
   4=Unknown</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="freq" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>The frequency rate used by the sonar for the detection.  Meaured in kHz.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="depth" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>The operationg depth, from the water surface, of the sonar at the time of the detction. Measured in Meters.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="pulseLength" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>The pulse length of the operating signal of a sonar at the time of the decteion.
Measured in Milliseconds.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute ref="latitude">
				<xsd:annotation>
					<xsd:documentation>The latitude location of the sonar at the time of the detection.  Measured in Decimal Degrees.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute ref="longitude">
				<xsd:annotation>
					<xsd:documentation>The latitude location of the sonar at the time of the detection.
Measured in Decimal Degrees.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="tilt">
				<xsd:annotation>
					<xsd:documentation>The angle from horizontal of the sonar at the time of the detection. Measured in  Angular-Degrees.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:float">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="90"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="pitch">
				<xsd:annotation>
					<xsd:documentation>The fore and aft angular rotation of the sonar platform's center of gravity at the time of detection. Measured in Angular-Degrees.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:float">
						<xsd:minInclusive value="-90.0"/>
						<xsd:maxInclusive value="90.0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="roll">
				<xsd:annotation>
					<xsd:documentation>The ATHWARTSHIPS angular rotation of the sonar platform's center of gravity  at the time of the detection.  Measured in Angular-Degrees.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:float">
						<xsd:minInclusive value="-90.0"/>
						<xsd:maxInclusive value="90.0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="yaw">
				<xsd:annotation>
					<xsd:documentation>The angular rotation about the vertical axis of the sonar platform's center of gravity at the time of the detection.  Meaured in Angular-Degrees.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:float">
						<xsd:minInclusive value="-90.0"/>
						<xsd:maxInclusive value="90.0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="contactBearing">
				<xsd:annotation>
					<xsd:documentation>The angle, measured from true North, of the contact from the sonar. Measured in Angular-Degrees.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:float">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="360"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="contactRange">
				<xsd:annotation>
					<xsd:documentation>The horizontal distance dimension of the contact from the sonar. Measured in Meters.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:float">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="9999.9"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Platform">
		<xsd:annotation>
			<xsd:documentation>The platform  parameters at the time of an observation of an object.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:attribute ref="latitude" use="optional"/>
			<xsd:attribute ref="longitude" use="optional"/>
			<xsd:attribute name="heading">
				<xsd:annotation>
					<xsd:documentation>The angle of the direction of movement, from true North, of the survey platform when the target was detected. </xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:float">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="360"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="equipType">
				<xsd:annotation>
					<xsd:documentation>The type of equipment used during the event to identify the object.
  0=EOD
  1=MNS
  2=MNV
  3=Sonar
  4=Other</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="navSystem">
				<xsd:annotation>
					<xsd:documentation>The code identifing the type of navigation system used.
   0=DM 40
   1=GPS
   2=Hyperfix
   3=Loran C
   4=Mini Ranger
   5=Syledis
   6=Other
   7=Unknown</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
						<xsd:enumeration value="5"/>
						<xsd:enumeration value="6"/>
						<xsd:enumeration value="7"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute ref="name">
				<xsd:annotation>
					<xsd:documentation>The name of the platform used in the survey.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="speed">
				<xsd:annotation>
					<xsd:documentation>The speed of the SURVEY platform for the at the time of the event. Measured in Knots.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:float">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="99.9"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="platformType">
				<xsd:annotation>
					<xsd:documentation>The code identifing the type of platform used in the event.
   0=EOD
   1=MCM
   2=MH53
   3=MHC
   4=White Hull
   5=Other</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:enumeration value="0"/>
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
						<xsd:enumeration value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Image">
		<xsd:annotation>
			<xsd:documentation>The parameters describing a snippet of imagery visually showing an observation of an object.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:attribute name="snippetFileName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The file name of the image snippet of the contact/target.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="scanlineLatitude" type="Latitude_Type">
				<xsd:annotation>
					<xsd:documentation>The latitude coordinate of the first pixel (upper right hand corner) of the snippet file. Measured in Decimal Degrees.
</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="scanlineLongitude" type="Longitude_Type">
				<xsd:annotation>
					<xsd:documentation>The longitude coordinate of the first pixel (upper right hand corner) of the snippet file. Measured in Decimal Degrees.
</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="scanlineRow" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>The row coordinate in the source imagery file corresponding to the first pixel (upper right corner) of the snippet. </xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="scanlineColumn" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>The column coordinate in the source imagery file corresponding to the first pixel (upper right corner) of the snippet. </xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="georefSnippetFilename" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Thye file name of a georeferenced image file (geoTiff, etc.) of the snippet. </xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="equipment" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>String describing the sonor used.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="MEDALName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The Image file name used in MEDAL. (GCCS Image Segment dependent).
</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="MEDALHost" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The host IP address for the Image Server. (MEDAL GCCS Dependent).</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="UNISIPSName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The UNISIPS IMage file name. </xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:simpleType name="sourceCode">
		<xsd:annotation>
			<xsd:documentation>Type used to capture the source (who recorded) of an environmental data parameter.
   0-CMWC (Commander, Mine Warfare Command)
   1-EOD (Explosive Odinance Diposal) Uinit
   2-HM (Helo squadron )
   3-MCMRON (Mine Warfare  Counte Measures Squadron)
   4-MCMV (Mine Counter Measures Vehicle)
   5-NAVOCEANO
   6-Other
   7-Unknown</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
			<xsd:enumeration value="3"/>
			<xsd:enumeration value="4"/>
			<xsd:enumeration value="5"/>
			<xsd:enumeration value="6"/>
			<xsd:enumeration value="7"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="sourceType">
		<xsd:annotation>
			<xsd:documentation>Type used to dertermine the source (where did it come from) of an environmental parameter.
   0-Measured
   1-Historical
   2-Chart</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
