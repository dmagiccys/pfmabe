							<xsd:annotation>
								<xsd:documentation>(Reuse from Joint METOC schema)</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="currentSpeedSource" type="sourceCode">
							<xsd:annotation>
								<xsd:documentation>The code identifing the source of the current data.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="currentSpeedType" type="sourceType">
							<xsd:annotation>
								<xsd:documentation>The code identifing the type of the current data.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="currentDirection">
							<xsd:annotation>
								<xsd:documentation>(Reuse from Joint METOC schema)</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="currentDirectionSource" type="sourceCode">
							<xsd:annotation>
								<xsd:documentation>The code identifing the source of the current data.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="currentDirectionType" type="sourceType">
							<xsd:annotation>
								<xsd:documentation>The code identifing the type of the current data.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="sspLatitude" type="Latitude_Type">
							<xsd:annotation>
								<xsd:documentation>The latitude at which the sound speed profile was taken. Measured in Degrees.</xsd:documentation>
